{
  "name": "create-bjerk-typescript",
  "version": "2.0.0",
  "private": false,
  "description": "Minimal typescript project template. Run yarn create bjerk-typescript <app name>",
  "keywords": [
    "nodejs",
    "nodejs-boilerplate",
    "typescript",
    "typescript-boilerplate",
    "starter",
    "boilerplate",
    "scaffold",
    "template"
  ],
  "repository": "github:bjerkio/create-bjerk-typescript",
  "license": "Apache-2.0",
  "author": "Bjerk AS",
  "type": "module",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "bin": {
    "create-bjerk-typescript": "./cmd/create-bjerk-typescript/index.cjs"
  },
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development dotenv-load nodemon src/main.ts",
