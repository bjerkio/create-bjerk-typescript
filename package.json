    "test": "jest --coverage src/**/*"
  },
  "prettier": "@cobraz/prettier",
  "eslintConfig": {
    "extends": "@bjerk/eslint-config"
  },
  "dependencies": {
    "fs-extra": "^10.0.1"
  },
  "devDependencies": {
    "@bjerk/eslint-config": "^2.0.1",
    "@cobraz/prettier": "^2.0.0",
    "@tsconfig/node16-strictest-esm": "^1.0.0",
    "@types/jest": "^28.1.5",
    "@types/node": "16",
    "dotenv-load": "^2.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.5.1",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.1.1",
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.17"
  }
