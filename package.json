{
  "name": "create-bjerk-typescript",
  "version": "1.0.2",
  "description": "Minimal typescript project template. Run yarn create bjerk-typescript <app name>",
  "type": "module",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "bin": {
    "create-bjerk-typescript": "./cmd/create-bjerk-typescript/index.cjs"
  },
  "repository": "github:bjerkio/create-bjerk-typescript",
  "author": "Bjerk AS",
  "keywords": [
    "nodejs",
    "nodejs-boilerplate",
    "typescript",
    "typescript-boilerplate",
    "starter",
    "boilerplate",
    "scaffold",
    "template"
  ],
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "dev": "tsnd --respawn src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage src/**/*",
    "build": "tsc"
  },
  "dependencies": {
    "fs-extra": "^10.0.1"
  },
  "prettier": "@cobraz/prettier",
  "eslintConfig": {
    "extends": "@bjerk/eslint-config"
  },
  "devDependencies": {
    "@bjerk/eslint-config": "^2.0.1",
    "@cobraz/prettier": "^2.0.0",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/jest": "^28.1.5",
    "@types/node": "18.0.4",
    "jest": "^28.1.3",
    "prettier": "^2.5.1",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@3.1.1",
  "volta": {
    "node": "16.14.0",
    "yarn": "1.22.17"
  }
}
